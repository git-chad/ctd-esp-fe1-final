{
  "source": {
    "include": [
      "./src/store/characterReducer.ts",
      "./src/store/detailsReducer.ts",
      "./src/store/favoritesReducer.ts"
    ],
    "includePattern": ".+\\.ts$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "recurse": true,
    "destination": "./docs"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-ts-import-plugin"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
